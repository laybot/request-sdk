{
  "name": "laybot/request-sdk",
  "description": "LayBot Â· Universal PHP HTTP request toolkit (Guzzle + Workerman + SSE)",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.8",
    "psr/http-message": "^1.0 || ^2.0",
    "workerman/workerman": "^4.1 || ^5.0",
    "phpunit/phpunit": "^9.6",
    "psr/log": "^3"
  },
  "suggest": {
    "workerman/workerman": "Enable coroutine transport for Webman/Swoole"
  },
  "autoload": {
    "psr-4": {
      "LayBot\\Request\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LayBot\\Request\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "LayBot",
      "email": "larry@laybot.cn"
    }
  ],
  "keywords": [
    "http",
    "guzzle",
    "workerman",
    "sse",
    "sdk",
    "laybot"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
